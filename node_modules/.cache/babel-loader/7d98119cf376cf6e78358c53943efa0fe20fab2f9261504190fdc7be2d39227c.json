{"ast":null,"code":"export default {\n  data() {\n    return {\n      searchQuery: \"\",\n      sortField: \"\",\n      sortOrder: 1,\n      foods: [{\n        id: 1,\n        name: \"Pizza Margherita\",\n        category: \"Main Course\",\n        status: \"Active\",\n        sales: 120,\n        stock: 50,\n        price: 12.99,\n        image: \"pizza-margherita.jpg\"\n      }, {\n        id: 2,\n        name: \"Spaghetti Carbonara\",\n        category: \"Main Course\",\n        status: \"Active\",\n        sales: 85,\n        stock: 30,\n        price: 14.99,\n        image: \"spaghetti-carbonara.jpg\"\n      }, {\n        id: 3,\n        name: \"Caesar Salad\",\n        category: \"Appetizer\",\n        status: \"Disabled\",\n        sales: 40,\n        stock: 10,\n        price: 9.99,\n        image: \"caesar-salad.jpg\"\n      }, {\n        id: 4,\n        name: \"Tiramisu\",\n        category: \"Dessert\",\n        status: \"Active\",\n        sales: 150,\n        stock: 20,\n        price: 6.99,\n        image: \"tiramisu.jpg\"\n      }\n      // Thêm các món ăn khác\n      ]\n    };\n  },\n  computed: {\n    filteredFoods() {\n      let filtered = this.foods.filter(food => food.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      if (this.sortField) {\n        filtered.sort((a, b) => {\n          let result = 0;\n          if (a[this.sortField] < b[this.sortField]) result = -1;\n          if (a[this.sortField] > b[this.sortField]) result = 1;\n          return result * this.sortOrder;\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    sortBy(field) {\n      if (this.sortField === field) {\n        this.sortOrder *= -1; // Đảo ngược thứ tự sắp xếp\n      } else {\n        this.sortField = field;\n        this.sortOrder = 1;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","searchQuery","sortField","sortOrder","foods","id","name","category","status","sales","stock","price","image","computed","filteredFoods","filtered","filter","food","toLowerCase","includes","sort","a","b","result","methods","sortBy","field"],"sources":["D:\\Tiểu Luận Tốt Nghiệp\\Frontend\\src\\components\\Admin\\Dish\\AdminDish.vue"],"sourcesContent":["<template>\n  <div class=\"food-management\">\n    <div class=\"header\">\n      <h2>Food Management</h2>\n      <input\n        type=\"text\"\n        placeholder=\"Search...\"\n        v-model=\"searchQuery\"\n        class=\"search\"\n      />\n      <button class=\"add-button\">Add Food</button>\n    </div>\n\n    <table class=\"food-table\">\n      <thead>\n        <tr>\n          <th @click=\"sortBy('name')\">Name ⬍</th>\n          <th @click=\"sortBy('category')\">Category ⬍</th>\n          <th @click=\"sortBy('status')\">Status ⬍</th>\n          <th @click=\"sortBy('sales')\">Sales ⬍</th>\n          <th @click=\"sortBy('stock')\">Stock ⬍</th>\n          <th @click=\"sortBy('price')\">Price ⬍</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"food in filteredFoods\" :key=\"food.id\">\n          <td>\n            <img :src=\"food.image\" alt=\"Food image\" class=\"food-image\" />\n            {{ food.name }}\n          </td>\n          <td>{{ food.category }}</td>\n          <td>\n            <span\n              :class=\"{\n                'active-status': food.status === 'Active',\n                'disabled-status': food.status === 'Disabled',\n              }\"\n            >\n              {{ food.status }}\n            </span>\n          </td>\n          <td>{{ food.sales }}</td>\n          <td>{{ food.stock }}</td>\n          <td>${{ food.price }}</td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      searchQuery: \"\",\n      sortField: \"\",\n      sortOrder: 1,\n      foods: [\n        {\n          id: 1,\n          name: \"Pizza Margherita\",\n          category: \"Main Course\",\n          status: \"Active\",\n          sales: 120,\n          stock: 50,\n          price: 12.99,\n          image: \"pizza-margherita.jpg\",\n        },\n        {\n          id: 2,\n          name: \"Spaghetti Carbonara\",\n          category: \"Main Course\",\n          status: \"Active\",\n          sales: 85,\n          stock: 30,\n          price: 14.99,\n          image: \"spaghetti-carbonara.jpg\",\n        },\n        {\n          id: 3,\n          name: \"Caesar Salad\",\n          category: \"Appetizer\",\n          status: \"Disabled\",\n          sales: 40,\n          stock: 10,\n          price: 9.99,\n          image: \"caesar-salad.jpg\",\n        },\n        {\n          id: 4,\n          name: \"Tiramisu\",\n          category: \"Dessert\",\n          status: \"Active\",\n          sales: 150,\n          stock: 20,\n          price: 6.99,\n          image: \"tiramisu.jpg\",\n        },\n        // Thêm các món ăn khác\n      ],\n    };\n  },\n  computed: {\n    filteredFoods() {\n      let filtered = this.foods.filter((food) =>\n        food.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n      );\n\n      if (this.sortField) {\n        filtered.sort((a, b) => {\n          let result = 0;\n          if (a[this.sortField] < b[this.sortField]) result = -1;\n          if (a[this.sortField] > b[this.sortField]) result = 1;\n          return result * this.sortOrder;\n        });\n      }\n\n      return filtered;\n    },\n  },\n  methods: {\n    sortBy(field) {\n      if (this.sortField === field) {\n        this.sortOrder *= -1; // Đảo ngược thứ tự sắp xếp\n      } else {\n        this.sortField = field;\n        this.sortOrder = 1;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.food-management {\n  padding: 20px;\n  background-color: #101827;\n  color: #fff;\n  width: 100%;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.search {\n  padding: 10px;\n  background-color: #1d283c;\n  border-radius: 5px;\n  border: none;\n  width: 200px;\n}\n\n.add-button {\n  background-color: #0084ff;\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.food-table {\n  width: 100%;\n  border-collapse: collapse;\n  text-align: left;\n}\n\n.food-table th,\n.food-table td {\n  padding: 10px;\n  border-bottom: 1px solid #444;\n}\n\n.food-table th {\n  cursor: pointer;\n}\n\n.food-image {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  margin-right: 10px;\n  vertical-align: middle;\n}\n\n.active-status {\n  color: #2;\n}\n\n.disabled-status {\n  color: #ff0000;\n}\n</style>\n"],"mappings":"AAmDA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,CAAC;MACZC,KAAK,EAAE,CACL;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,kBAAkB;QACxBC,QAAQ,EAAE,aAAa;QACvBC,MAAM,EAAE,QAAQ;QAChBC,KAAK,EAAE,GAAG;QACVC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,KAAK;QACZC,KAAK,EAAE;MACT,CAAC,EACD;QACEP,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,qBAAqB;QAC3BC,QAAQ,EAAE,aAAa;QACvBC,MAAM,EAAE,QAAQ;QAChBC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,KAAK;QACZC,KAAK,EAAE;MACT,CAAC,EACD;QACEP,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,cAAc;QACpBC,QAAQ,EAAE,WAAW;QACrBC,MAAM,EAAE,UAAU;QAClBC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,IAAI;QACXC,KAAK,EAAE;MACT,CAAC,EACD;QACEP,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,UAAU;QAChBC,QAAQ,EAAE,SAAS;QACnBC,MAAM,EAAE,QAAQ;QAChBC,KAAK,EAAE,GAAG;QACVC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,IAAI;QACXC,KAAK,EAAE;MACT;MACA;MAAA;IAEJ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,IAAIC,QAAO,GAAI,IAAI,CAACX,KAAK,CAACY,MAAM,CAAEC,IAAI,IACpCA,IAAI,CAACX,IAAI,CAACY,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAAClB,WAAW,CAACiB,WAAW,CAAC,CAAC,CACjE,CAAC;MAED,IAAI,IAAI,CAAChB,SAAS,EAAE;QAClBa,QAAQ,CAACK,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;UACtB,IAAIC,MAAK,GAAI,CAAC;UACd,IAAIF,CAAC,CAAC,IAAI,CAACnB,SAAS,IAAIoB,CAAC,CAAC,IAAI,CAACpB,SAAS,CAAC,EAAEqB,MAAK,GAAI,CAAC,CAAC;UACtD,IAAIF,CAAC,CAAC,IAAI,CAACnB,SAAS,IAAIoB,CAAC,CAAC,IAAI,