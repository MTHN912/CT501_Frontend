{"ast":null,"code":"import axios from \"axios\";\nimport { computed, onMounted } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"Navbar\",\n  data() {\n    return {\n      cart: {\n        items: []\n      }\n    };\n  },\n  computed: {\n    // Tính tổng số lượng món trong giỏ hàng\n    cartTotalQuantity() {\n      return this.cart.items.reduce((total, item) => total + item.quantity, 0);\n    },\n    // Kiểm tra trạng thái đăng nhập\n    isLoggedIn() {\n      return this.$store.getters.isLoggedIn;\n    },\n    // Kiểm tra quyền admin của người dùng\n    isAdmin() {\n      const userInfo = this.$store.getters.userInfo;\n      return userInfo?.ROLE?.IS_ADMIN;\n    }\n  },\n  methods: {\n    // Lấy dữ liệu giỏ hàng từ server\n    async fetchCart() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/cart/getCart\");\n        this.cart = response.data.cart;\n      } catch (error) {\n        console.error(\"Lỗi khi tải giỏ hàng:\", error);\n      }\n    },\n    // Hàm đăng xuất\n    logout() {\n      localStorage.removeItem(\"token\");\n      this.$router.push(\"/login\");\n    }\n  },\n  mounted() {\n    // Tải giỏ hàng khi component được mount\n    this.fetchCart();\n  }\n};","map":{"version":3,"names":["axios","computed","onMounted","useRouter","useStore","name","data","cart","items","cartTotalQuantity","reduce","total","item","quantity","isLoggedIn","$store","getters","isAdmin","userInfo","ROLE","IS_ADMIN","methods","fetchCart","response","get","error","console","logout","localStorage","removeItem","$router","push","mounted"],"sources":["D:\\Tiểu Luận Tốt Nghiệp\\Frontend\\src\\components\\Layout\\Navbar.vue"],"sourcesContent":["<template>\n  <div class=\"container-fluid nav-bar\">\n    <div class=\"container\">\n      <nav class=\"navbar navbar-light navbar-expand-lg py-4\">\n        <router-link to=\"/\" class=\"navbar-brand\">\n          <h1 class=\"text-primary f