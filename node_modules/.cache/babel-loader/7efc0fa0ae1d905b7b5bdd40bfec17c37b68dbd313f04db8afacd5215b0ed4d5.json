ks {\n  display: flex;\n  justify-content: space-between;\n  margin: 15px 0;\n  font-size: 13px;\n}\n\n.form-links a {\n  color: #007bff;\n  text-decoration: none;\n}\n\n.divider {\n  text-align: center;\n  margin: 15px 0;\n  font-size: 12px;\n  color: #888;\n}\n\n.social-login {\n  display: flex;\n  justify-content: space-between;\n}\n\n.btn-social {\n  width: 48%;\n  padding: 12px;\n  font-size: 14px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.btn-social font-awesome-icon {\n  margin-right: 10px;\n  font-size: 20px;\n}\n\n.btn-facebook {\n  background-color: #3b5998;\n  color: white;\n}\n\n.btn-google {\n  background-color: #db4437;\n  color: white;\n}\n\n.btn-facebook:hover {\n  background-color: #2d4373;\n}\n\n.btn-google:hover {\n  background-color: #c23321;\n}\n\n.form-footer {\n  margin-top: 20px;\n  text-align: center;\n}\n\n.form-footer p {\n  font-size: 13px;\n}\n\n.form-footer a {\n  color: #007bff;\n}\n</style>\n"],"mappings":"AAqEA,SAASA,eAAe,QAAQ,8BAA8B;AAC9D,SAASC,GAAG,QAAQ,KAAK;AACzB,SAASC,SAAS,QAAQ,YAAY;;;;;;;IAEtC,MAAMC,SAAS,GAAGF,GAAG,CAAC;MACpBG,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,iBAAiB,GAAGL,GAAG,CAAC,KAAK,CAAC;IAEpC,MAAMM,MAAM,GAAGL,SAAS,CAAC,CAAC;IAE1B,MAAMM,YAAY,GAAGA,CAAA,KAAM;MACzB,IAAIL,SAAS,CAACM,KAAK,CAACL,QAAQ,IAAID,SAAS,CAACM,KAAK,CAACJ,QAAQ,EAAE;QACxDK,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAER,SAAS,CAACM,KAAK,CAACL,QAAQ,CAAC;QACvDM,OAAO,CAACC,GAAG,CAAC,WAAW,EAAER,SAAS,CAACM,KAAK,CAACJ,QAAQ,CAAC;QAClDE,MAAM,CAACK,IAAI,CAAC,GAAG,CAAC;MAClB,CAAC,MAAM;QACLC,KAAK,CAAC,gCAAgC,CAAC;MACzC;IACF,CAAC;IAED,MAAMC,wBAAwB,GAAGA,CAAA,KAAM;MACrCR,iBAAiB,CAACG,KAAK,GAAG,CAACH,iBAAiB,CAACG,KAAK;IACpD,CAAC;IAED,MAAMM,mBAAmB,GAAGA,CAAA,KAAM;MAChCL,OAAO,CAACC,GAAG,CAAC,qBAAqB,CAAC;MAClC;IACF,CAAC;IAED,MAAMK,iBAAiB,GAAGA,CAAA,KAAM;MAC9BN,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;MAChC;IACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDe